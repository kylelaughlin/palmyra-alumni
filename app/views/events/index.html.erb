<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.name, event_path(event) %></td>
        <td><%= event.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if current_classmate.admin? %>
  <%= link_to 'New Event', new_event_path %>
<% end %>
